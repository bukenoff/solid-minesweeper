{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "tauri-test",
  "mainBinaryName": "tauri-test",
  "version": "0.1.0",
  "identifier": "com.tauri-test.app",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "minesweeper",
        "width": 240,
        "height": 320,
        "maxWidth": 740,
        "maxHeight": 480,
        "fullscreen": false,
        "resizable": false,
        "useHttpsScheme": true
      }
    ]
  }
}